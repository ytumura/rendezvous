<%= render :partial => "/_parts/links" %>

<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Tags:</strong>
  <%= @post.tag_list.join(', ') %>
</p>

<p>
  <strong>Users:</strong>
  <%= @post.user_list.join(', ') %>
</p>

<p>
  <strong>Content:</strong>
  <%= @post.content %>
</p>
